{
  "compilerOptions": {
    "baseUrl": ".",
    "module": "ESNext",
    "target": "es2016",
    "lib": [
      "DOM",
      "ESNext",
      "es2018"
    ],
    "strict": true,
    "esModuleInterop": true,
    "incremental": false,
    "skipLibCheck": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "noUnusedLocals": true,
    "strictNullChecks": true,
    "forceConsistentCasingInFileNames": true,
    "types": [
      "vite/client",
      "vite-plugin-pages/client",
      "vite-plugin-vue-layouts/client"
    ],
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@myTypes/*": [
        "./src/@types/*"
      ],
      "@services/*": [
        "./src/services/*"
      ],
      "@icons/*": [
        "./src/assets/icons/*"
      ],
      "@stores/*": [
        "./src/stores/*"
      ],
      "@atoms/*": [
        "./src/components/atoms/*"
      ],
      "@molecules/*": [
        "./src/components/molecules/*"
      ],
      "@organisms/*": [
        "./src/components/organisms/*"
      ],
      "@templates/*": [
        "./src/components/templates/*"
      ]
    }
  },
  "exclude": [
    "dist",
    "node_modules"
  ]
}